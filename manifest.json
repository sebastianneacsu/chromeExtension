{
	"name": "paThor",
	"manifest_version": 2,
	"description": "CSS selector and xPath validator",
	"version": "0.1",
	"background": {
		"page": "background.html"
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "paThor"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["script.js", "contentscript.js"]
		}
	],
	"permissions": [
		"tabs"
	],
	"web_accessible_resources": ["executeQuery.js"]
}